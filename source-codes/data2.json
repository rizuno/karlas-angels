    "forms3": {
        "html": "\r\n    <style>\r\n        \r\n        .row {\r\n            background-color: cyan;\r\n        }\r\n\r\n        label {\r\n            display: inline-block;\r\n            width: 100px;\r\n            text-align: left;\r\n        }\r\n    </style>\r\n\r\n    <div class=\"container\" style=\"display: flex; align-items: left; justify-content: center;\">\r\n        <form action=\"submit.html\" method=\"POST\" style=\"background-color: cyan\">   \r\n        <div class=\"row\">\r\n            <label for=\"pname\">Project Name</label>\r\n            <br>\r\n            <input type=\"text\" id=\"pname\" name=\"pname\" placeholder=\"project name\" required>\r\n        </div>\r\n\r\n        <br>\r\n\r\n        <div class=\"row\">\r\n            <label for=\"assign\">Assigned to</label>\r\n            <br>\r\n            <select name=\"text\" id=\"text\" placeholder=\"Ex. Merry Pelision\" required>\r\n                <option>Francesca Tengco</option>\r\n                <option>Felise Sanchez</option>\r\n                <option>Christian Villafuerte</option>\r\n                <option>Jedd Fernando</option>\r\n            </select>\r\n        </div>\r\n\r\n        <br>\r\n\r\n        <div class=\"row\">\r\n            <label for=\"sdate\">Start Date</label>\r\n            <br>\r\n            <input type=\"date\" id=\"sdate\" name=\"sdate\">\r\n        </div>\r\n\r\n        <br>\r\n\r\n        <div class=\"row\">\r\n            <label for=\"edate\">End Date</label>\r\n            <br>\r\n            <input type=\"date\" id=\"edate\" name=\"edate\">\r\n        </div>\r\n\r\n        <br>\r\n\r\n        <div class=\"row\">\r\n            <label for=\"priority\">Priority</label>\r\n            <br>\r\n            <input type=\"radio\" id=\"high\" name=\"priority\" value=\"high\">\r\n            <label for=\"high\">High</label>\r\n\r\n            <input type=\"radio\" id=\"average\" name=\"priority\" value=\"average\">\r\n            <label for=\"average\">Average