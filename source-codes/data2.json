    "goodvibes": {
        "html": "\r\n        <style>\r\n\r\n            .container              {background-color: #A9D7E9;\r\n                                    height: 620px;}\r\n\r\n            .header                 {background-color: black;\r\n                                    height: 40px;}\r\n\r\n            .box                    {justify-content: center;\r\n                                    height: 930px;\r\n                                    width: 700px;}\r\n\r\n            form                    {background-color: #F3F3F3;\r\n                                    font-family: Arial, Helvetica, sans-serif;}\r\n\r\n            h4                      {text-align: center;\r\n                                    padding-top: 15px;}\r\n\r\n            .name                   {padding-left: 250px;}\r\n\r\n            .email                  {padding-left: 250px;}\r\n\r\n            .scale                  {padding-left: 40px;}\r\n\r\n            .place                  {padding-left: 138px;}\r\n \r\n            .time                   {padding-left: 20px;}\r\n\r\n            .emotion                {padding-left: 50px;}\r\n\r\n            .select                 {padding-left: 138px;}\r\n\r\n            .experience             {padding-left: 5px;}\r\n\r\n            #afternoon,\r\n            #evening                {margin-left: 297px;}\r\n\r\n            #elated,\r\n            #loved,\r\n            #Enthusiastic           {margin-left: 295px;}\r\n\r\n            #name,\r\n            #scale,\r\n            #email,\r\n            #place,\r\n            #exp                    {width: 270px;\r\n                                     border-radius: 4px;\r\n                                     border-color: #dedede;}\r\n\r\n            #exp                    {padding-bottom: 30px;}\r\n\r\n                             \r\n\r\n\r\n        </style>\r\n    \r\n        <div class=\"header\"></div>\r\n        <div class=\"container\" style=\"display: flex; align-items: left; justify-content: center;\">\r\n            \r\n                <div class=\"box\">\r\n                    <h1 style=\"text-align: center;\">Good Vibes Form</h1>\r\n                    <form>\r\n                        <h4>Tell us something positive that happened to you today</h4><br>\r\n\r\n                        <label class=\"name\" for=\"name\">Name</label>\r\n                        <input type=\"text\" id=\"name\" name=\"name\" placeholder=\"Enter your name\" required><br><br>\r\n\r\n                        <label class=\"email\" for=\"email\">Email</label>\r\n                        <input type=\"email\" id=\"email\" name=\"email\" placeholder=\"Enter your email address\" required><br><br>\r\n\r\n                        <label class=\"scale\" for=\"scale\">On scale of 1-10, how good was it?</label>\r\n                        <input type=\"number\" id=\"scale\" name=\"scale\" min=\"1\" max=\"10\" placeholder=\"Enter a number between 1 and 10\" required><br><br>\r\n                        \r\n                        <label class=\"place\" for=\"place\"> Where did it happen?</label>\r\n                        <select name=\"place\" id=\"place\" required>\r\n                            <option>At home</option>\r\n                            <option>At park</option>\r\n                            <option>At school</option>\r\n                            <option>At work</option>\r\n                        </select><br><br>\r\n\r\n                        <label class=\"time\" for=\"time\">At what time of the day did it happen?</label>\r\n                        <input type=\"radio\" id=\"morning\" name=\"morning\" value=\"morning\">\r\n                        <label for=\"morning\">Morning</label><br>\r\n\r\n                        <input type=\"radio\" id=\"afternoon\" name=\"afternoon\" value=\"afternoon\">\r\n                        <label for=\"afternoon\">Afternoon</label><br>\r\n\r\n                        <input type=\"radio\" id=\"evening\" name=\"evening\" value=\"evening\">\r\n                        <label for=\"evening\">Evening</label><br><br>\r\n\r\n\r\n                        <label class=\"emotion\" for=\"emotion\">What emotion did you experince?</label>\r\n                        <input type=\"checkbox\" id=\"excited\" name=\"excited\" value=\"excited\">\r\n                        <label for=\"excited\">Excited</label><br>\r\n\r\n                        <label class=\"select\" for=\"select\">(Select all that apply)</label>\r\n                        <input type=\"checkbox\" id=\"humbled\" name=\"humbled\" value=\"humbled\">\r\n                        <label for=\"humbled\">Humbled</label><br>\r\n\r\n                        <input type=\"checkbox\" id=\"elated\" name=\"elated\" value=\"elated\">\r\n                        <label for=\"elated\">Elated</label><br>\r\n\r\n                        <input type=\"checkbox\" id=\"loved\" name=\"loved\" value=\"loved\">\r\n                        <label for=\"loved\">Loved</label><br>\r\n\r\n                        <input type=\"checkbox\" id=\"Enthusiastic\" name=\"Enthusiastic\" value=\"Enthusiastic\">\r\n                        <label for=\"Enthusiastic\">Enthusiastic</label><br><br>\r\n\r\n                        <label class=\"experience\" for=\"experience\">Please describe your positive experience</label>\r\n                        <input type=\"text\" id=\"exp\" name=\"exp\"\r\n                         placeholder=\"Enter your experience here...\"><br><br>\r\n                        \r\n                        <center>\r\n                        <input type=\"submit\" style=\"background-color: #6BA2CE; color: white; border-style: none; padding: 10px;\"><br><br>\r\n                        </center>\r\n                    </form>\r\n                </div>\r\n            \r\n        </div>\r\n  \r\n"
    },
