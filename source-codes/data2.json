  "billing": {
    "html": "<style>\r\n    #container1 {\r\n        justify-items: center;\r\n        align-items: center;\r\n        display: flex;\r\n        margin: auto;\r\n        background-color: white;\r\n    }\r\n    \r\n    table {\r\n        border: 2px solid;\r\n        background-color: white;\r\n        border-collapse: collapse;\r\n    }\r\n    \r\n    tr {\r\n        text-align: left;\r\n        background-color: white;\r\n        font-family: Arial, Helvetica, sans-serif;\r\n    }\r\n    \r\n    th,\r\n    td {\r\n        padding-right: 30px;\r\n        background-color: white;\r\n    }\r\n    \r\n    #qty,\r\n    #at {\r\n        background-color: white;\r\n        padding-right: 100px;\r\n    }\r\n    \r\n    .cell {\r\n        border: 1px solid;\r\n        background-color: white;\r\n    }\r\n    \r\n    .mergel {\r\n        border-right: 1px solid;\r\n        background-color: white;\r\n    }\r\n    \r\n    .box,\r\n    #subt,\r\n    #tax,\r\n    #grandt {\r\n        background-color: white;\r\n    }\r\n</style>\r\n\r\n<div id=\"container1\">\r\n    <table>\r\n        <tr class=\"box\">\r\n            <th class=\"cell\" colspan=\"3\" id=\"invo\">Invoice #123456789</th>\r\n            <th class=\"cell\">14 January 2025</th>\r\n        </tr>\r\n\r\n        <tr>\r\n            <th class=\"mergel\" colspan=\"2\" id=\"payt\">Pay to:</th>\r\n            <th class=\"merge\" colspan=\"2\" id=\"customer\">Cutomer:</th>\r\n        </tr>\r\n\r\n        <tr>\r\n            <td class=\"mergel\" colspan=\"2\" id=\"acme\">Acme Billing Co.</td>\r\n            <td class=\"merge\" colspan=\"2\" id=\"smith\">John Smith</td>\r\n        </tr>\r\n\r\n        <tr>\r\n            <td class=\"mergel\" colspan=\"2\" id=\"mainst\">123 Main St.</td>\r\n            <td class=\"merge\" colspan=\"2\" id=\"willow\">321 Willow Way</td>\r\n        </tr>\r\n\r\n        <tr>\r\n            <td class=\"mergel\" colspan=\"2\" id=\"cityvil\">Cityville, NA 12345</td>\r\n            <td class=\"merge\" colspan=\"2\" id=\"south\">Southeast Northwestershrine, MA 54321</td>\r\n        </tr>\r\n\r\n        <tr class=\"box\">\r\n            <th class=\"cell\" id=\"desc\">Name/ Description</th>\r\n            <th class=\"cell\" id=\"qty\">Qty.</th>\r\n            <th class=\"cell\" id=\"at\">@</th>\r\n            <th class=\"cell\" id=\"cost\">Cost</th>\r\n        </tr>\r\n\r\n        <tr>\r\n            <td class=\"cell\" id=\"paperc\">Paperclips</td>\r\n            <td class=\"cell\" id=\"price1\">1000</td>\r\n            <td class=\"cell\" id=\"price2\">0.01</td>\r\n            <td class=\"cell\" id=\"price3\">10.00</td>\r\n        </tr>\r\n\r\n        <tr>\r\n            <td class=\"cell\" id=\"staples\">Staples (box)</td>\r\n            <td class=\"cell\" id=\"price4\">100</td>\r\n            <td class=\"cell\" id=\"price5\">1.00</td>\r\n            <td class=\"cell\" id=\"price6\">100.00</td>\r\n        </tr>\r\n\r\n        <tr>\r\n            <th class=\"cell\" colspan=\"3\" id=\"subt\">Subtotal</th>\r\n            <td class=\"cell\" id=\"invo\">110.00</td>\r\n        </tr>\r\n\r\n        <tr>\r\n            <th class=\"cell\" colspan=\"2\" id=\"tax\">Tax</th>\r\n            <td class=\"cell\" id=\"perc\">8%</td>\r\n            <td class=\"cell\" id=\"taxpr\">8.80</td>\r\n        </tr>\r\n\r\n        <tr>\r\n            <th class=\"cell\" colspan=\"3\" id=\"gtotal\">Grand Total</th>\r\n            <td class=\"cell\" id=\"total\">$118.80</td>\r\n        </tr>\r\n    </table>\r\n</div>"
  },
